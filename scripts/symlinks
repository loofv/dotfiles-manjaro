# !bin/sh
ln -s -f ~/git/dotfiles-manjaro/init.vim ~/.config/nvim/init.vim
ln -s -f ~/git/dotfiles-manjaro/i3config ~/.config/i3/config
ln -s -f ~/git/dotfiles-manjaro/.aliases ~/.aliases
ln -s -f ~/git/dotfiles-manjaro/.zshrc ~/.zshrc
touch ~/.zsh_prompt
ln -s -f ~/git/dotfiles-manjaro/.zsh_prompt ~/.zsh_prompt
ln -s -f ~/git/dotfiles-manjaro/scripts/startup ~/.custom_scripts/startup
#ln -s -f ~/git/dotfiles-manjaro/alacritty.yml ~/.config/alacritty/alacritty.yml
#ln -s -f ~/git/dotfiles-manjaro/lovehtheme1.vim ~/.config/nvim/colors/lovehtheme1.vim

